<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ClearableEditText">
        <attr name="cet_inputType" format="enum">
            <!-- 输入类型为普通文本 -->
            <enum name="text" value="1" />
            <!-- 密码 -->
            <enum name="password" value="2" />
        </attr>
        <attr name="cet_textColor" format="color" />
        <attr name="cet_textSize" format="dimension" />
        <attr name="cet_editTextPadding" format="dimension" />
        <attr name="cet_buttonWidth" format="dimension" />
        <attr name="cet_buttonPadding" format="dimension" />
        <attr name="cet_buttonSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="ShopCartPlusMinus">
        <attr name="scpm_buttonWidth" format="dimension" />
    </declare-styleable>

    <!-- AutoPhotoLayout -->
    <declare-styleable name="camera_flow_layout">
        <attr name="max_count" format="integer" />
        <attr name="item_margin" format="integer" />
        <!-- 一行有多少个 -->
        <attr name="line_count" format="integer" />

    </declare-styleable>

    <!-- ImageSelector -->
    <declare-styleable name="ImageSelector">
        <!-- 添加图片的图标 -->
        <attr name="is_add_icon" format="reference" />
        <!-- 动态计算高度时，需要剪掉的部分 -->
        <attr name="is_image_margin" format="integer" />
        <!-- 列数 -->
        <attr name="is_column_count" format="integer" />
        <!-- 最大张数 -->
        <attr name="is_max_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="ValidateSeekbar">
        <attr name="vs_text" format="string" />
        <attr name="vs_text_color" format="color" />
        <attr name="vs_text_size" format="dimension" />
    </declare-styleable>
</resources>